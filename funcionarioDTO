public class FuncionarioDTO {
    private int id;
    private String nome;
    private String cpf;
    private String email;
    private String cargo;
    private String telefone;
    private String turno;

    // Construtor vazio

    /**
     *
     *
     * @param id
     * @param nome
     * @param cpf
     * @param email
     * @param telefone
     * @param cargo
     * @param turno
     */
   
    // Construtor completo
    public FuncionarioDTO(int id, String nome, String cpf, String email, String cargo, String telefone, String turno) {
        this.id = id;
        this.nome = nome;
        this.cpf = cpf;
        this.email = email;
        this.cargo = cargo;
        this.telefone = telefone;
        this.turno = turno;
    }

    public FuncionarioDTO(String nome, String cpf, String email, String cargo, String telefone, String turno) {
        this.nome = nome;
        this.cpf = cpf;
        this.email = email;
        this.cargo = cargo;
        this.telefone = telefone;
        this.turno = turno;
    }
   
   

    // Getters e Setters
    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getNome() {
        return nome;
    }

    public void setNome(String nome) {
        this.nome = nome;
    }

    public String getCpf() {
        return cpf;
    }

    public void setCpf(String cpf) {
        this.cpf = cpf;
    }

    public String getEmail() {
        return email;
    }

    public void setEmail(String email) {
        this.email = email;
    }

    public String getCargo() {
        return cargo;
    }

    public void setCargo(String cargo) {
        this.cargo = cargo;
    }

    public String getTelefone() {
        return telefone;
    }

    public void setTelefone(String telefone) {
        this.telefone = telefone;
    }

    public String getTurno() {
        return turno;
    }

    public void setTurno(String turno) {
        this.turno = turno;
    }
}
